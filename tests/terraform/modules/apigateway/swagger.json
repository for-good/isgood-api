{
  "openapi": "3.0.1",
  "info": {
    "title": "exampleoas",
    "version": "0.1.5"
  },
  "paths": {
    "/lambda": {
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": "${lambdaUri}",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",

          "httpMethod": "POST",
          "type": "aws_proxy"
        },
        "security": [
          {
              "ExampleRegisteredUsers": []
          }
        ],
        "responses": {
          "default": {
            "$ref": ""
          }
        }
      }
    },
    "/echo": {
      "get": {
        "x-amazon-apigateway-integration": {
          "uri": "${echoUri}",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "connectionType": "INTERNET",
          "httpMethod": "GET",
          "type": "http_proxy"
        },
        "responses": {
          "default": {
            "$ref": ""
          }
        }
      },
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": "${echoUri}",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "connectionType": "INTERNET",
          "httpMethod": "POST",
          "type": "http_proxy"
        },
        "responses": {
          "default": {
            "$ref": ""
          }
        }
      }

    }
  },
  "components": {
  "securitySchemes": {
    "ExampleRegisteredUsers": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "x-amazon-apigateway-authtype": "cognito_user_pools",
      "x-amazon-apigateway-authorizer": {
        "type": "cognito_user_pools",
        "providerARNs": [
          "${userPoolArn}"
        ]
      }
    }
  }
  }

}
