{
  "openapi": "3.0.1",
  "info": {
    "title": "exampleoas",
    "version": "0.1.6"
  },
  "paths": {
    "/lambda": {
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": "${lambdaUri}",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",

          "httpMethod": "POST",
          "type": "aws_proxy"
        },
        "security": [
          {
              "ExampleRegisteredUsers": []
          }
        ],
        "responses": {
          "200": {
            "content": { "application/json": { "schema": { "$ref": "#/components/schemas/ExampleModel" } } }
          }
        }
      }
    },
    "/echo": {
      "get": {
        "x-amazon-apigateway-integration": {
          "uri": "${echoUri}",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "connectionType": "INTERNET",
          "httpMethod": "GET",
          "type": "http_proxy"
        },
        "responses": {
          "200": {
            "content": { "application/json": { "schema": { "$ref": "#/components/schemas/ExampleModel" } } }

        }
      },
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": "${echoUri}",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "connectionType": "INTERNET",
          "httpMethod": "POST",
          "type": "http_proxy"
        },
        "responses": {
          "200": {
            "content": { "application/json": { "schema": { "$ref": "#/components/schemas/ExampleModel" } } }

        }
      }

    }
  },
  "components": {
 
    "schemas": {
      "ExampleModel": {
        "type": "object",
        "properties": {
            "code": {
              "type": "integer",
              "format": "int32"
            },
            "message": {
              "type": "string"
            }
        }
    }
  },

  "securitySchemes": {
    "ExampleRegisteredUsers": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "x-amazon-apigateway-authtype": "cognito_user_pools",
      "x-amazon-apigateway-authorizer": {
        "type": "cognito_user_pools",
        "providerARNs": [
          "${userPoolArn}"
        ]
      }
    }
  }
  }

}
