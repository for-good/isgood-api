{
  "openapi": "3.0.1",
  "info": {
    "title": "exampleapi",
    "description": "Example API",
    "version": "0.1.8"
  },


  "paths": {
    "/echo": {
      "get": {
        "x-amazon-apigateway-integration": {
          "uri": ${echoUri},
          "httpMethod": "GET",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "http_proxy"
        }
      },
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": ${echoUri},
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "http_proxy"
        }
      }
    },
    "/lambda": {
      "post": {
        "security": [
          {
            "ExampleRegisteredUsers": []
          }
        ],
        "x-amazon-apigateway-integration": {
          "uri": ${lambdaUri},
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "aws_proxy"
        }
      }
    }
  },
  "components": {
    "securitySchemes": {
      "ExampleRegisteredUsers": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header",
        "x-amazon-apigateway-authtype": "cognito_user_pools",
        "x-amazon-apigateway-authorizer": {
          "providerARNs": [
            ${userpoolArn}
          ],
          "type": "cognito_user_pools"
        }
      }
    }
  }
}
