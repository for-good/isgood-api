{
  "openapi": "3.0.1",
  "info": {
    "title": "exampleapi",
    "description": "Example API",
    "version": "0.1.7"
  },

  "paths": {
    "/echo": {
      "get": {
        "x-amazon-apigateway-integration": {
          "uri": ${echoUri},
          "httpMethod": "GET",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "http_proxy"
        },
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": ${echoUri},
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "http_proxy"
        }
      }


      }

    }

  },

  "components": {
    "securitySchemes": {
      "ExampleRegisteredUsers": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header",
        "x-amazon-apigateway-authtype": "cognito_user_pools",
        "x-amazon-apigateway-authorizer": {
          "providerARNs": [
            ${userPoolArn}
          ],
          "type": "cognito_user_pools"
        }
      }
    }
  }
}
