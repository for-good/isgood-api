{
  "openapi": "3.0.1",
  "info": {
    "title": "exampleapi",
    "description": "Example API",
    "version": "0.1.9"
  },

  "paths": {

    "/lambda": {
      "post": {
        "security": [
          {
              "ExampleRegisteredUsers": []
          }
        ],

        "x-amazon-apigateway-integration": {
	  "uri": "${lambda_uri}",
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "aws_proxy"
        }
      }
    },

    "/lambda/mu": {
      "post": {
        "security": [
          {
              "ExampleRegisteredUsers": []
          }
        ],
        "x-amazon-apigateway-integration": {
          "uri": "${lambda_uri}",
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "aws_proxy"
        }
      }
    },


    "/echo": {
      "get": {
        "x-amazon-apigateway-integration": {
          "uri": "${echo_uri}",
          "httpMethod": "GET",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "http_proxy"
        }
      },

      "post": {
        "x-amazon-apigateway-integration": {
          "uri": "${echo_uri}",
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "http_proxy"
        }
      }

    },

    "/echo/foxtrot": {
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": "${foxtrot_uri}",
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "http_proxy"
        }
      }
    },

    "/muck": {
      "post": {
        "x-amazon-apigateway-integration": {
          "uri": "${lambda_uri}",
          "httpMethod": "POST",
          "responses": {
            "default": {
              "statusCode": "200"
            }
          },
          "passthroughBehavior": "when_no_match",
          "type": "aws_proxy"
        }
      }
    }
  },

  "components": {
  "securitySchemes": {
    "ExampleRegisteredUsers": {
      "type": "apiKey",
      "name": "Authorization",
      "in": "header",
      "x-amazon-apigateway-authtype": "cognito_user_pools",
      "x-amazon-apigateway-authorizer": {
        "type": "cognito_user_pools",
        "providerARNs": [
          "${userPoolArn}"
        ]
      }
    }
  }
  }
}
